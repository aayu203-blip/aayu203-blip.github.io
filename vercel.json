{
  "rewrites": [
    {
      "source": "/:brand(volvo|scania|komatsu|cat|hitachi|kobelco)/:category/:part",
      "destination": "/pages/products/:brand/:category/:part.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
